{
  "name": "json-schema-editor-visual",
  "version": "2.0.0",
  "description": "jsonschema editor",
  "main": "dist/main.umd.js",
  "module": "dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.js",
      "default": "./package/index.js"
    },
    "./dist/main.css": "./dist/main.css"
  },
  "license": "MIC",
  "scripts": {
    "start": "webpack serve --mode development --port 8082 --open",
    "demo": "cross-env NODE_ENV=production webpack --mode production",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:esm": "cross-env NODE_ENV=production webpack --config webpack.esm.config.js --mode production",
    "build:all": "npm run build && npm run build:esm",
    "build:prd": "webpack --config webpack.prd.config.js --mode production",
    "build:prd:dev": "webpack --config webpack.prd.config.js --mode development",
    "serve": "webpack serve --config webpack.dev.config.js --mode development --port 8082"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "antd": "^5.22.0",
    "brace": "^0.10.0",
    "generate-schema": "^2.6.0",
    "json-schema-editor-visual": "^2.0.0",
    "moox": "^1.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^4.1.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.9.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
